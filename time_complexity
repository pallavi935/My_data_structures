//Time complexity o(n)
#include <stdio.h>
#include <time.h>

int main() {
    int n = 10;  // you can change this value
    clock_t start, end;
    double cpu_time_used;

    start = clock();  // start timer

    for (int i = 1; i <= n; i++) {
        printf("%d\n", i);
    }

    end = clock();  // end timer
    cpu_time_used = ((double)(end - start)) / CLOCKS_PER_SEC;

    printf("Execution time: %f seconds\n", cpu_time_used);

    return 0;
}


//Time complexity
#include <stdio.h>
#include <time.h>
    int main() {
    int n = 1000,i,j; 
    clock_t start, end;
    double cpu_time_used;
    start = clock(); 
    for (i = 1; i <= n; i++) {
        for (j = 1; j <= n; j++); 
    }
        printf("the loop executed %d times \n", i * j);
    end = clock();
    cpu_time_used = ((double)(end - start)) / CLOCKS_PER_SEC;
    printf("Execution time: %f seconds\n", cpu_time_used);
    return 0;
}
