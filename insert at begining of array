#include<stdio.h>
#define CAP 100
int main(){
    int a[CAP]={1,2,3};
    int n=3;
    int value=12;
    if(n<CAP){
        for (int i=n;i>0;i--){
        a[i]=a[i-1];
        }
        a[0]=value;
        n++;
    for(int i=0;i<n;i++)
    printf("%d ",a[i]);
}
else
    printf("Array is full\n");
}
